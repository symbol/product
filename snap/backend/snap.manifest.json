{
  "version": "0.1.0",
  "description": "Manage Symbol mosaics, and connect to Symbol apps with MetaMask.",
  "proposedName": "Symbol",
  "repository": {
    "type": "git",
    "url": "https://github.com/symbol/product.git"
  },
  "source": {
    "shasum": "a+hD1aSKN53ZQKDdKt78ij8hblQMVmY1yyGaJDgyHZk=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "snap-symbol",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "snap_manageState": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "fetchCurrencyPrice"
          }
        },
        {
          "expression": "* * * * *",
          "request": {
            "method": "fetchFeeMultiplier"
          }
        }
      ]
    },
    "endowment:network-access": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "snap_getBip44Entropy": [
      {
        "coinType": 1
      },
      {
        "coinType": 4343
      }
    ]
  },
  "manifestVersion": "0.1"
}
