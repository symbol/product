{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/api/*": ["api/*"],
			"@/components/*": ["components/*"],
			"@/config": ["config/index.js"],
			"@/constants": ["constants/index.js"],
			"@/contexts/*": ["contexts/*"],
			"@/pages/*": ["pages/*"],
			"@/public/*": ["public/*"],
			"@/styles/*": ["styles/*"],
			"@/utils": ["utils"],
			"@/utils/*": ["utils/*"]
		}
	},
	"exclude": ["**/node_modules/*", "**/build/*", "**/scripts/*"]
}
